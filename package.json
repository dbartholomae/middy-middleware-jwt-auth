    "husky": "^7.0.2",
    "jest": "^27.5.1",
    "lint-staged": "^12.1.3",
    "moment": "^2.27.0",
    "pkg-ok": "^2.3.1",
    "prettier-standard": "^16.4.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "serverless": "^3.4.0",
    "serverless-offline": "^8.5.0",
    "serverless-webpack": "^5.6.1",
    "source-map-support": "^0.5.19",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
