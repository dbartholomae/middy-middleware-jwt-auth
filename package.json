{
  "name": "middy-middleware-jwt-auth",
  "version": "6.0.0",
  "description": "A middy JSON web token authorization middleware inspired by express-jwt.",
  "homepage": "",
  "license": "MIT",
  "author": {
    "name": "Daniel Bartholomae",
    "email": "daniel@bartholomae.name",
    "url": ""
  },
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "keywords": [
    "middy",
    "middleware",
    "jwt",
    "json web token"
  ],
  "types": "lib/index.d.ts",
  "engines": {
    "npm": ">= 4.0.0"
  },
  "private": false,
  "dependencies": {
    "debug": "^4.1.1",
    "http-errors": "^2.0.0",
