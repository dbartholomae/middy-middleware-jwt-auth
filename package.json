    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.3.1",
    "moment": "^2.29.4",
    "pkg-ok": "^3.0.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.0",
    "semantic-release": "^23.0.0",
    "serverless": "^3.25.0",
    "serverless-offline": "^13.3.3",
    "serverless-webpack": "^5.11.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
